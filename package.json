{
  "name": "fam-trainingsplan",
  "version": "2.4.0",
  "type": "module",
  "description": "Interaktiver Trainingsplan für Free Arts of Movement - Parkour, Trampolin, Tricking in München",
  "author": "Free Arts of Movement",
  "license": "MIT",
  "keywords": [
    "trainingsplan",
    "parkour",
    "trampolin",
    "tricking",
    "munich",
    "fam",
    "pwa",
    "tailwind-v4",
    "alpine-js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "vitest run --project unit",
    "test:unit:watch": "vitest --project unit",
    "test:unit:ui": "vitest --ui --project unit",
    "test:integration": "playwright test --config=playwright.integration.config.js",
    "test:integration:ui": "playwright test --config=playwright.integration.config.js --ui",
    "test:integration:debug": "playwright test --config=playwright.integration.config.js --debug",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.js",
    "test:visual:update": "playwright test tests/e2e/visual-regression.spec.js --update-snapshots",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.js",
    "test:perf": "playwright test tests/e2e/performance.spec.js",
    "test:pwa": "playwright test tests/e2e/pwa-offline.spec.js",
    "test:flows": "playwright test tests/e2e/user-flows.spec.js",
    "test:puppeteer": "node tests/puppeteer/run-tests.js",
    "test:puppeteer:headless": "node tests/puppeteer/run-tests.js --headless",
    "test:puppeteer:slow": "node tests/puppeteer/run-tests.js --slow",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,css,html}\""
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.15.0",
    "@alpinejs/focus": "^3.15.0",
    "@alpinejs/intersect": "^3.15.0",
    "@alpinejs/persist": "^3.15.0",
    "alpinejs": "^3.15.0",
    "fuse.js": "^7.0.0",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.0",
    "@types/alpinejs": "^3.13.11",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.8.1",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.15.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.3.3",
    "puppeteer": "^24.23.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.8",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "Safari >= 16.4",
    "Chrome >= 111",
    "Firefox >= 128"
  ]
}
